class Main {
    
    public static int func(int r, int c) {
        int res = 1;
        for(int i = 0; i < c; i++) {
            res *= (r-i);
            res /= (i+1);
        }
        return res;
    }
    
    public static void main(String[] args) {
        int r = 6;
        int c = 1;
        System.out.print(func(r-1, c-1));
    }
}
